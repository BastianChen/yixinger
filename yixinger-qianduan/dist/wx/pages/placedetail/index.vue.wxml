          <view class="_div data-v-8962503e commentDetails">
            <view class="_div data-v-8962503e commentDetail" wx:key="commentListInfoIndex" key="{{commentListInfoIndex}}" wx:for="{{commentListInfo}}" wx:for-index="commentListInfoIndex" wx:for-item="comment">
              <van-row class="_van-row data-v-8962503e">
