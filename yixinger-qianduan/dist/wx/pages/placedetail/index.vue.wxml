        <view class="_div data-v-8962503e two"><label class="_span data-v-8962503e otherSpan">{{telephone1}}</label> <label class="_span data-v-8962503e centerSpan">|</label> <label class="_span data-v-8962503e otherSpan">{{telephone2}}</label></view>
