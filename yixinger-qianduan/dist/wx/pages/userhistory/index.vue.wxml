<template name="6e7749dc">
  <view class="_div data-v-2ecb44f2 index">
    <van-tabs active="{{active}}" bindchange="handleProxy" sticky animated swipeable color="#00BFFF" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_van-tabs data-v-2ecb44f2">
      <van-tab title="景点" class="_van-tab data-v-2ecb44f2">
        <view class="_div data-v-2ecb44f2 firstDiv" wx:if="{{isSceneryDataEmpty}}">
          <view class="_div data-v-2ecb44f2">
            <image src="/static/images/noContent.png" class="_img data-v-2ecb44f2"></image>
          </view>
          <view class="_div data-v-2ecb44f2 title"><label class="_span data-v-2ecb44f2"> 暂无推荐景点 </label></view>
        </view>
        <view wx:key="index" key="{{index}}" class="_div data-v-2ecb44f2 secondDiv" wx:if="{{!isSceneryDataEmpty}}" bindtap="handleProxy" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{sceneryData}}" wx:for-index="index" wx:for-item="item">
          <view class="_div data-v-2ecb44f2">
            <view class="_div data-v-2ecb44f2 rowBottom">
              <van-row class="_van-row data-v-2ecb44f2">
                <van-col span="4" offset="1" class="_van-col data-v-2ecb44f2">
                  <image src="{{item.image}}" class="_img data-v-2ecb44f2"></image>
                </van-col>
                <van-col span="15" offset="4" class="_van-col data-v-2ecb44f2">
                  <view class="_div data-v-2ecb44f2 secondCol">
                    <view class="_div data-v-2ecb44f2 nameDiv">
                      {{item.placeName}}
                    </view>
                    <view class="_div data-v-2ecb44f2 distanceAndAddressDiv">
                      {{item.distance}}|{{item.address}}
                    </view>
                    <view class="_div data-v-2ecb44f2 overallratingAndShowTagDiv">
                      <van-row hidden="{{!(item.overallRating)}}" class="_van-row data-v-2ecb44f2">
                        <van-col span="11" class="_van-col data-v-2ecb44f2">
                          <van-rate value="{{item.overallRating}}" size="15" count="5" disabled-color="#00BFFF" disabled class="_van-rate data-v-2ecb44f2"></van-rate>
                        </van-col>
                        <van-col span="3" class="_van-col data-v-2ecb44f2">
                          {{item.overallRating}}
                        </van-col>
                      </van-row>
                      <view class="_div data-v-2ecb44f2 tagPaddingTop" hidden="{{!(item.showtag)}}">
                        <van-tag color="#f2826a" plain size="large" class="_van-tag data-v-2ecb44f2">{{item.showtag}}</van-tag>
                      </view>
                    </view>
                  </view>
                </van-col>
              </van-row>
            </view>
          </view>
        </view>
      </van-tab>
      <van-tab title="餐馆" class="_van-tab data-v-2ecb44f2">
        <view class="_div data-v-2ecb44f2 firstDiv" wx:if="{{isRestaurantDataEmpty}}">
          <view class="_div data-v-2ecb44f2">
            <image src="/static/images/noContent.png" class="_img data-v-2ecb44f2"></image>
          </view>
          <view class="_div data-v-2ecb44f2 title"><label class="_span data-v-2ecb44f2"> 暂无推荐餐馆 </label></view>
        </view>
        <view wx:key="index" key="{{index}}" class="_div data-v-2ecb44f2 secondDiv" bindtap="handleProxy" wx:if="{{!isRestaurantDataEmpty}}" data-eventid="{{'1-'+index}}" data-comkey="{{$k}}" wx:for="{{restaurantData}}" wx:for-index="index" wx:for-item="item">
          <view class="_div data-v-2ecb44f2">
            <view class="_div data-v-2ecb44f2 rowBottom">
              <van-row class="_van-row data-v-2ecb44f2">
                <van-col span="4" offset="1" class="_van-col data-v-2ecb44f2">
                  <image src="{{item.image}}" class="_img data-v-2ecb44f2"></image>
                </van-col>
                <van-col span="15" offset="4" class="_van-col data-v-2ecb44f2">
                  <view class="_div data-v-2ecb44f2 secondCol">
                    <view class="_div data-v-2ecb44f2 nameDiv">
                      {{item.placeName}}
                    </view>
                    <view class="_div data-v-2ecb44f2 distanceAndAddressDiv">
                      {{item.distance}}|{{item.address}}
                    </view>
                    <view class="_div data-v-2ecb44f2 overallratingAndShowTagDiv">
                      <van-row class="_van-row data-v-2ecb44f2">
                        <van-col span="11" class="_van-col data-v-2ecb44f2">
                          <van-rate value="{{item.overallRating}}" size="15" count="5" disabled-color="#00BFFF" disabled class="_van-rate data-v-2ecb44f2"></van-rate>
                        </van-col>
                        <van-col span="3" class="_van-col data-v-2ecb44f2">
                          {{item.overallRating}}
                        </van-col>
                      </van-row>
                      <view class="_div data-v-2ecb44f2 tagPaddingTop"><label wx:if="{{item.price||item.price!=''}}" class="_span data-v-2ecb44f2">¥{{item.price}}</label>
                        <van-tag color="#f2826a" plain size="large" class="_van-tag data-v-2ecb44f2">{{item.showtag}}</van-tag>
                      </view>
                    </view>
                  </view>
                </van-col>
              </van-row>
            </view>
          </view>
        </view>
      </van-tab>
    </van-tabs>
  </view>
</template>